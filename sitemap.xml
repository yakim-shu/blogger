layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% capture site.url %}{% endcapture %}

  <!-- Posts -->
  {% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ post.url | xml_escape }}</loc>
  </url>
  {% endfor %}
</urlset>